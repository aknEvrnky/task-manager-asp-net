@model IEnumerable<string>

<script>
    let errors = @Html.Raw(Json.Serialize(Model));
    
    errors.forEach(error => Toastify({
        text: error,
        duration: 5000,
        close: true,
        gravity: "top",
        position: "right",
        backgroundColor: "red",
        stopOnFocus: true,
    }).showToast());
    
</script>
